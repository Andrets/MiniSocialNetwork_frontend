{
	"trailingComma": "none",
	"tabWidth": 2,
	"useTabs": true,
	"semi": false,
	"singleQuote": true,
	"jsxSingleQuote": true,
	"arrowParens": "avoid",
	"singleAttributePerLine": true,
	"importOrder": [
		"<THIRD_PARTY_MODULES>",
		"@/components/(.*)$",
		"@/layout/(.*)$",
		"@/ui/(.*)$",
		"@/providers/(.*)$",
		"@/constants/(.*)$",
		"@/schemas/(.*)$",
		"@/shared/(.*)$",
		"@/assets/(.*)$",
		"@/config/(.*)$",
		"@/store/(.*)$",
		"@/hooks/(.*)$",
		"@/utils/(.*)$",
		"@/api/(.*)$",
		"^../(.*)",
		"^./(.*)",
		"(.scss)$"
	],
	"importOrderSeparation": true,
	"importOrderSortSpecifiers": true
}
